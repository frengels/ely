#ifndef TOK
#define TOK(id, name)
#endif

TOK(whitespace, "ws")
TOK(tab, "\\t")
TOK(newline_lf, "\\n")
TOK(newline_cr, "\\r")
TOK(newline_crlf, "\\r\\n")

TOK(lparen, "(")
TOK(rparen, ")")
TOK(lbracket, "[")
TOK(rbracket, "]")
TOK(lbrace, "{")
TOK(rbrace, "}")

TOK(identifier, "id")

TOK(integer_lit, "int")
TOK(decimal_lit, "dec")
TOK(string_lit, "str")

TOK(unterminated_string_lit, "unterminated str")
TOK(unknown, "unknown")

TOK(eof, "EOF")

#undef TOK