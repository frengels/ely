#ifndef TOKEN
#error TOKEN(_) must be defined
#endif

TOKEN(whitespace)
TOKEN(tab)
TOKEN(newline_lf)
TOKEN(newline_cr)
TOKEN(newline_crlf)
TOKEN(line_comment)
TOKEN(block_comment)

TOKEN(lparen)
TOKEN(rparen)
TOKEN(lbracket)
TOKEN(rbracket)
TOKEN(lbrace)
TOKEN(rbrace)

TOKEN(quote)
TOKEN(quasiquote)
TOKEN(unquote)
TOKEN(unquote_splicing)
TOKEN(syntax)
TOKEN(quasisyntax)
TOKEN(unsyntax)
TOKEN(unsyntax_splicing)

TOKEN(dollar)
TOKEN(slash)

TOKEN(identifier)
TOKEN(integer_lit)
TOKEN(decimal_lit)
TOKEN(string_lit)

TOKEN(unterminated_string_lit)
TOKEN(unknown)

// these 3 are the different events the lexer can emit as a last token in the encoded stream

// indicates that '\0' was found and the file is done lexing
TOKEN(eof)
// indicate this should be spilled and the next received token will 
// indicate which this is part of. [... spill], [identifier ...] -> [identifier ...].
// if present in the buffer it's always as the last token
TOKEN(spill)
// to signal that the buffer needs to be refreshed
// also always the final token in the buffer
TOKEN(buffer_full)

#undef TOKEN
#undef TOKEN2
